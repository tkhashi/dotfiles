{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base_background": "#18354c",
    "base_foreground": "#ffc107",
    "git": "#ff8f07"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:base_background",
          "foreground": "p:base_foreground",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .PWD }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "p:base_background",
          "foreground": "p:git",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:base_foreground",
          "foreground": "p:base_background",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "foreground": "p:base_background",
          "background": "p:base_foreground",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#fff",
          "background": "p:base_background",
          "style": "plain",
          "template": " \uf64f {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#fff",
          "background": "p:base_background",
          "style": "plain",
          "template": "|",
          "type": "text"
        },
        {
          "foreground": "#fff",
          "background": "p:base_background",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \ufa1e{{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:base_foreground",
          "style": "plain",
          "template": " \u25b6",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
} 
